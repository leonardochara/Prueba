<html>

<head>
	<title>Alumnos</title>
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO"
	crossorigin="anonymous">
</head>

<body>
		

		<div class="container">
				<div class="row">
					<div class="col-md-9 mt-4 text-center">
						<table class="table table-striped table-bordered">
							<tr>
								<th>DNI</th>
								<th>APELLIDOS Y NOMBRES</th>
								<th>COLEGIO</th>
								<th>PROVINCIA</th>
								<th>NOTA</th>
								<th>FOTO</th>
							</tr>

							
							<% for(var i = 0 ; i < alumnos.length; i++){ %>
							<tr>
								<td>
								<%= alumnos[i].DNI %>
								</td>

								<td>
								<%= alumnos[i].apeNom %>
								</td>
								
								<td>
								<%= alumnos[i].colegio %>
								</td>
								
								<td>
								<%= alumnos[i].provincia %>
								</td>

								<td>
								<%= alumnos[i].nota %>
								</td>

								<td>
								

								<!--<img src="data:image/jpeg;charset=utf-8;binary,<%= alumnos[i].foto %>" width="50%">-->
								<img src="/ver?DNI=<%= alumnos[i].DNI %>" width="75%">
								
								</td>

								<td>
									<a href="/editar?id=<%= alumnos[i].DNI%>">Editar</a>
								</td>

							</tr>
							
							<% } %>

						</table>
					</div>
				</div>
		</div>
</body>
</html>